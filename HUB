Tab:AddToggle({
Name = "FS 1B",
Default = false,
Callback = function(state)
       fsLoop = state

        if fsLoop then
            -- Teleportieren
            local hrp = game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
            if hrp then
                hrp.CFrame = CFrame.new(1176, 4789, -2293)
            end

            fsThread = task.spawn(function()
                while fsLoop do
                    task.wait(0.1)
                    game.ReplicatedStorage.RemoteEvent:FireServer({"+FS4"})
                end
            end)
        else
            -- Loop wird gestoppt beim Toggle-Off
            fsLoop = false
        end
    end
})
