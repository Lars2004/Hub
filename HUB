    Default = false,
    Callback = function(toggle)
        kek = toggle
        task.spawn(function()
            while kek do
                local Remote = game.ReplicatedStorage:WaitForChild("RemoteEvent")
                local player = game:GetService("Players"):FindFirstChild(PlayerChosen)

                if player and player.Character and player.Character:FindFirstChild("UpperTorso") then
                    local position = player.Character.UpperTorso.Position

                    local Arguments1 = {
                        {
                            "Skill_BulletPunch",
                            "Right",
                            position
                        }
                    }

                    Remote:FireServer(unpack(Arguments1))

                    wait(0.1)

                    local Arguments2 = {
                        {
                            "Skill_BulletPunch",
                            "Left",
                            position
                        }
                    }

                    Remote:FireServer(unpack(Arguments2))
                end

                wait(0.3)
            end
        end)
    end
})
