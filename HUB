Tab:AddToggle({
    Name = "TP Spam PP",
    Default = false,
    Callback = function(state)
        kek = state

        local Players = game:GetService("Players")
        local RunService = game:GetService("RunService")
        local LocalPlayer = Players.LocalPlayer

        local function startTPFollow()
            -- Safety first
            if tpLoopConnection then
                tpLoopConnection:Disconnect()
                tpLoopConnection = nil
            end

            local targetPlayer = Players:FindFirstChild(PlayerChosen)
            if not targetPlayer then
                warn("Spieler nicht gefunden: " .. tostring(PlayerChosen))
                return
            end

            local function getCharacters()
                local myChar = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
                local targetChar = targetPlayer.Character or targetPlayer.CharacterAdded:Wait()
                return myChar, targetChar
            end

            local myChar, targetChar = getCharacters()
            local myRoot = myChar:WaitForChild("HumanoidRootPart")
            local targetRoot = targetChar:WaitForChild("HumanoidRootPart")

            -- RenderStep Loop starten
            tpLoopConnection = RunService.RenderStepped:Connect(function()
                if kek and myRoot and targetRoot then
                    local pos = targetRoot.Position + Vector3.new(0, 30, 0)
                    myRoot.Velocity = Vector3.zero
                    myRoot.CFrame = CFrame.new(pos)
                end
            end)
        end

        local function stopTPFollow()
            -- Setze den Charakter auf den Boden zurück
            local myChar = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
            local myRoot = myChar:WaitForChild("HumanoidRootPart")
            myRoot.CFrame = CFrame.new(myRoot.Position.X, 0, myRoot.Position.Z)  -- Position auf Boden setzen

            -- Trenne die Verbindungen
            if tpLoopConnection then
                tpLoopConnection:Disconnect()
                tpLoopConnection = nil
            end
            if respawnConnection then
                respawnConnection:Disconnect()
                respawnConnection = nil
            end
        end

        if kek then
            startTPFollow()

            -- Listener für Respawn
            respawnConnection = LocalPlayer.CharacterAdded:Connect(function()
                task.wait(1) -- kurz warten, bis alles geladen ist
                if kek then
                    startTPFollow()
                end
            end)
        else
            stopTPFollow()
        end
    end
})

