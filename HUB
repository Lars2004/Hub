local Section = Tab:AddSection({
Name = "Remove Player"
})
local PlayerChosen;
local drop = Tab:AddDropdown({
Name = "Player",
Default = game.Players.LocalPlayer.Name,
Options = playerlist,
Callback = function(new)
PlayerChosen = new;
end
})
Tab:AddToggle({
    Name = "Completely Remove Player",
    Default = false,
    Callback = function(state)
        local targetPlayer = game.Players:FindFirstChild(PlayerChosen)
        if targetPlayer and targetPlayer ~= game.Players.LocalPlayer then

            -- Funktion um Character zu entfernen
            local function removeCharacter()
                if targetPlayer.Character and targetPlayer.Character:IsDescendantOf(workspace) then
                    targetPlayer.Character.Parent = nil
                end
            end

            -- Funktion um Character zurückzugeben
            local function restoreCharacter()
                if targetPlayer.Character and not targetPlayer.Character:IsDescendantOf(workspace) then
                    targetPlayer.Character.Parent = workspace
                end
            end

            -- Wenn aktiv: Entfernen + Listener setzen
            if state then
                removeCharacter()

                -- Wenn schon ein Connection existiert, disconnecten
                if characterConnections[targetPlayer] then
                    characterConnections[targetPlayer]:Disconnect()
                end

                -- Neue Connection für zukünftige Spawns
                characterConnections[targetPlayer] = targetPlayer.CharacterAdded:Connect(function(char)
                    task.wait(0.1)
                    char.Parent = nil
                end)
            else
                -- Deaktiviert: Zurückbringen
                restoreCharacter()

                -- Listener entfernen
                if characterConnections[targetPlayer] then
                    characterConnections[targetPlayer]:Disconnect()
                    characterConnections[targetPlayer] = nil
                end
            end
        end
    end
})
